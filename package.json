  "name": "mern-ecommerce",
  "version": "1.0.0",
  "description": "photoshop",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node backend/server",
    "server": "nodemon backend/server",
    "client": "npm start --prefix frotend",
